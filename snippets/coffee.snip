snippet server
    http = require "http"
    fs = require "fs"
    path = require "path"
    http.createServer (req,res)->
        url = req.url
        console.log url
        url = url.substr(1)
        console.log url
        if !fs.existsSync(url)
            url = "index.html"
        try
            ext = path.extname(url)
            res.end fs.readFileSync(url,"binary"),"binary"
        catch
            res.end ""
        return
    .listen(80)
    console.log "start..."
